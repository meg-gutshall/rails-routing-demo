<h1>All the Songs!</h1>

<ul>
  <li>URL: <%= songs_url %></li>
  <li>PATH: <%= songs_path %></li>
</ul>

<ul>
<% @songs.each do |song| %>
  <ul>
    <li><b><i><%=song.title%></i></b> by <b><%=song.artist.name%></b></li>
    <ul>
    </ul>
    <%= link_to "Go to song", artist_song_path(song.artist, song) %>
  </ul>
<% end %>
</ul>


